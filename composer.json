{
  "name": "gwack/core",
  "description": "The gwack framework",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fathi Helmi",
      "email": "fathi@libfoo.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "symfony/http-foundation": "7.3.0",
    "psr/log": "3.0.2",
    "psr/container": "2.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "monolog/monolog": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Gwack\\": "src/Gwack"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "apcu-autoloader": true
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}
